{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "images": {
    "sizes": [200, 400],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "*.airtableusercontent.com"
      },
      {
        "protocol": "https",
        "hostname": "v5.airtableusercontent.com"
      }
    ],
    "formats": ["image/webp"],
    "minimumCacheTTL": 86400
  }
}
